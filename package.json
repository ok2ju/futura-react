{
  "name": "futura-react",
  "packageManager": "yarn@3.2.4",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "packages:build": "parcel build 'packages/*/*/' --no-cache && yarn packages:styles",
    "packages:styles": "yarn workspaces foreach run build:tailwind",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "parcel": "^2.7.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  }
}
